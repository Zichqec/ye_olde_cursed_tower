function tenday
{
	return Random.Select([
		"tenday",
		"decad",
		"fortnight",
		"sennight",
		"triennium",
		"{Random.GetIndex(1, 16)} moons",
		"quarter year",
	]);
}

function liveyourlife
{
	return Random.Select([
		"live your life",
		"do whatever it is you deem important",
		"tend to your garden",
		"capture a new soul for your collection",
	]);
}